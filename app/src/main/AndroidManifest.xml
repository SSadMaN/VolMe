<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sadman.volme">

    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityLauncher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityHome" />
        <activity android:name=".ActivityAddingEventForm" />
        <activity android:name=".ActivityProfile" />
        <activity
            android:name=".ActivityBonuses"
            android:label="@string/title_activity_bonuses"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ActivityEvent"
            android:label="@string/title_activity_event"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ActivitySubscriberProfile"
            android:label="@string/sub_profile"
            android:theme="@style/AppTheme" />
        <activity android:name=".ListSubscribers" />
    </application>

</manifest>